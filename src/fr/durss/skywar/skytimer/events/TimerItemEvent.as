package fr.durss.skywar.skytimer.events {	import flash.events.Event;	/**	 * Event fired by the <code>TimerItem</code> class.	 * 	 * @author Fran√ßois	 */	public class TimerItemEvent extends Event {		public static const LOOP:String			= "loop";		public static const PAUSE:String		= "pause";		public static const CLOSE:String		= "close";		public static const EDIT:String			= "edit";		public static const COMPLETE:String		= "complete";		public static const CHANGE_SOUND:String	= "changeSound";		public static const SHOW_TOOLTIP:String	= "showToolTip";		public static const HIDE_TOOLTIP:String	= "hideToolTip";		public static const SHOW_PLUG_IN:String	= "showPlugInToolTip";		public static const SHOW_PLUG_OUT:String= "showPlugOutToolTip";		public static const HIDE_PLUG:String	= "hidePlugToolTip";						/* *********** *		 * CONSTRUCTOR *		 * *********** */		public function TimerItemEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false) {			super(type, bubbles, cancelable);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/* ****** *		 * PUBLIC *		 * ****** */		override public function clone():Event { return new TimerItemEvent(type, bubbles, cancelable); }						/* ******* *		 * PRIVATE *		 * ******* */	}}